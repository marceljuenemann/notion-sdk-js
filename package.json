{
  "name": "@notionhq/client",
  "version": "1.0.4",
  "description": "A simple and easy to use client for the Notion API",
  "engines": {
    "node": ">=12"
  },
  "homepage": "https://developers.notion.com/docs/getting-started",
  "bugs": {
    "url": "https://github.com/makenotion/notion-sdk-js/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/makenotion/notion-sdk-js/"
  },
  "keywords": [
    "notion",
    "notionapi",
    "rest",
    "notion-api"
  ],
  "main": "./build/src",
  "types": "./build/src/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "prepublishOnly": "npm run checkLoggedIn && npm run lint && npm run test",
    "build": "tsc",
    "prettier": "prettier --write .",
    "lint": "prettier --check . && eslint . --ext .ts && cspell '**/*' ",
    "test": "ava",
    "check-links": "git ls-files | grep md$ | xargs -n 1 markdown-link-check",
    "prebuild": "npm run clean",
    "clean": "rm -rf ./build",
    "checkLoggedIn": "./scripts/verifyLoggedIn.sh",
    "build-docs-markdown": "npx ts-node scripts/apiExtractorWithExportedTypes.ts",
    "local-docs-build": "npm run build-docs-markdown && ./scripts/buildDocsHtml.sh",
    "local-docs-serve": "http-server docs-build/html -o"
  },
  "author": "",
  "license": "MIT",
  "files": [
    "build/package.json",
    "build/src/**"
  ],
  "dependencies": {
    "@types/node-fetch": "^2.5.10",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@ava/typescript": "^2.0.0",
    "@microsoft/api-documenter": "^7.18.0",
    "@microsoft/api-extractor": "^7.28.2",
    "@types/fs-extra": "^9.0.13",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "ava": "^3.15.0",
    "cspell": "^5.4.1",
    "eslint": "^7.24.0",
    "fs-extra": "^10.1.0",
    "http-server": "^14.1.1",
    "markdown-link-check": "^3.8.7",
    "prettier": "^2.3.0",
    "ts-node": "^10.8.1",
    "typescript": "^4.2.4"
  }
}
